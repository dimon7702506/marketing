<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Oborot" table="oborot">
    <unique-constraints>
      <unique-constraint name="oborot_id" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tovId" type="integer" column="tov_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="saldoKol" type="float" column="saldo_kol" precision="10" scale="0" nullable="false"/>
    <field name="aptekaId" type="integer" column="apteka_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="prihodKol" type="float" column="prihod_kol" precision="10" scale="0" nullable="false"/>
    <field name="rashodKol" type="float" column="rashod_kol" precision="10" scale="0" nullable="false"/>
    <field name="price" type="float" column="price" precision="10" scale="0" nullable="false"/>
    <field name="sum" type="float" column="sum" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
