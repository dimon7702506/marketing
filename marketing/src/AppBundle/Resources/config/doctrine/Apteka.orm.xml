<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Apteka" table="apteka">
    <unique-constraints>
      <unique-constraint name="apteka_id" columns="apteka_id"/>
      <unique-constraint name="ip_adres" columns="ip_adres"/>
    </unique-constraints>
    <id name="aptekaId" type="integer" column="apteka_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adres" type="string" column="adres" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateAct" type="datetime" column="date_act" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="ipAdres" type="string" column="ip_adres" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dbName" type="string" column="db_name" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
